{
  "name": "@atomico/store",
  "description": "Store to manage predictable and functional asynchrony",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/atomicojs/store.git"
  },
  "bugs": {
    "url": "https://github.com/atomicojs/store/issues"
  },
  "homepage": "https://github.com/atomicojs/store#readme",
  "author": "UpperCod",
  "scripts": {
    "start": "vite --force",
    "build": "vite build --outDir docs --base store",
    "test": "vitest ",
    "coverage": "vitest run --coverage",
    "exports": "exports \"src/*.ts\" --exports --types --analyzer  --main store",
    "prepublishOnly": "npm run coverage && npm run exports"
  },
  "dependencies": {
    "@atomico/hooks": "^3.33.4",
    "@uppercod/consume-generator": "^1.4.1",
    "atomico": "^1.62.1"
  },
  "devDependencies": {
    "@atomico/exports": "^0.27.0",
    "@atomico/plugin-vite": "^1.1.1",
    "c8": "^7.11.0",
    "formilk": "^0.20.0",
    "happy-dom": "^2.55.0",
    "typescript": "^4.6.3",
    "vite": "latest",
    "vitest": "^0.9.3"
  }
}
